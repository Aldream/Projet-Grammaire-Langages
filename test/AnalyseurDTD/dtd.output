Grammar

    0 $accept: main $end

    1 main: dtd_list_opt

    2 ident: IDENT
    3      | IDENT COLON IDENT

    4 dtd_list_opt: dtd_list_opt ATTLIST ident att_definition_opt CLOSE
    5             | dtd_list_opt ELEMENT ident contentspec CLOSE
    6             | /* empty */

    7 att_definition_opt: att_definition_opt attribute
    8                   | /* empty */

    9 attribute: ident att_type default_declaration

   10 att_type: CDATA
   11         | TOKENTYPE
   12         | enumerate

   13 enumerate: OPENPAR enum_list_plus CLOSEPAR

   14 enum_list_plus: enum_list PIPE item_enum

   15 enum_list: item_enum
   16          | enum_list PIPE item_enum

   17 item_enum: ident

   18 default_declaration: DECLARATION
   19                    | STRING
   20                    | FIXED STRING

   21 contentspec: EMPTY
   22            | ANY
   23            | children
   24            | mixed

   25 card_opt: QMARK
   26         | AST
   27         | PLUS
   28         | /* empty */

   29 children: choice card_opt
   30         | seq card_opt

   31 cp: children
   32   | IDENT card_opt

   33 choice: OPENPAR cp list_choice CLOSEPAR

   34 list_choice: list_choice PIPE cp
   35            | PIPE cp

   36 seq: OPENPAR cp list_seq_opt CLOSEPAR

   37 list_seq_opt: list_seq_opt COMMA cp
   38             | /* empty */

   39 mixed: OPENPAR PCDATA list_mixed CLOSEPAR AST
   40      | OPENPAR PCDATA CLOSEPAR ast_opt

   41 ast_opt: AST
   42        | /* empty */

   43 list_mixed: list_mixed PIPE IDENT
   44           | PIPE IDENT


